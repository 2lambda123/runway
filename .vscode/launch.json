{
    "configurations": [
        {
            "args": [
                "plan",
                "--debug"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/tmp/samplerepo/",
            "env": {
                "DEPLOY_ENVIRONMENT": "dev"
            },
            "justMyCode": true,
            "name": "Runway: Plan",
            "program": "${workspaceFolder}/.venv/bin/runway",
            "request": "launch",
            "type": "python"
        },
        {
            "args": [
                "deploy",
                "--debug"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/tmp/samplerepo/",
            "env": {
                "DEPLOY_ENVIRONMENT": "dev"
            },
            "justMyCode": true,
            "name": "Runway: Deploy",
            "program": "${workspaceFolder}/.venv/bin/runway",
            "request": "launch",
            "type": "python"
        },
        {
            "args": [
                "destroy",
                "--debug"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/tmp/samplerepo/",
            "env": {
                "DEPLOY_ENVIRONMENT": "dev"
            },
            "justMyCode": true,
            "name": "Runway: Destroy",
            "program": "${workspaceFolder}/.venv/bin/runway",
            "request": "launch",
            "type": "python"
        },
        {
            "args": [
                "--dist=loadfile",
                "--integration",
                "--numprocesses=auto"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "name": "Pytest",
            "program": "${workspaceFolder}/.venv/bin/pytest",
            "request": "launch",
            "type": "python"
        }
    ],
    "version": "0.2.0"
}
